<p id="notice"><%= notice %></p>

<h1>Listing Atms</h1>

<table>
  <thead>
    <tr>
      <th>Location</th>
      <th>References</th>
      <th>Bank</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @atms.each do |atm| %>
      <tr>
        <td><%= atm.location %></td>
        <td><%= atm.references %></td>
        <td><%= atm.bank %></td>
        <td><%= link_to 'Show', atm %></td>
        <td><%= link_to 'Edit', edit_atm_path(atm) %></td>
        <td><%= link_to 'Destroy', atm, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Atm', new_atm_path %>
