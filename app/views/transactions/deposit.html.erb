<h1>Deposit Transaction Only </h1>
<%= form_for :transaction, url: deposit_create_transactions_path do |f|%>

  <%= f.text_field :bank_id, value: @bank.id %><br><br>  
  <%= f.hidden_field :atm_id, value: @atm.id %>
  <%= f.text_field :cust_id, value: @cust.id %><br><br>

  Atm Number : <%= f.text_field :atm_no %><br><br>
  Atm PIN : <%= f.password_field :atm_pin %><br><br>
  Deposit Amonunt : <%= f.text_field :balance %><br><br>
  <%= f.submit "Submit"%>
<% end -%>